.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
  border: 1px solid green;
  margin: 10rpx;
}
.log-item {
  margin: 10rpx;
}
.content{
  width: 100%;
  margin:25rpx auto;
  background: white;
}
.content .title{
  width: 160rpx;
  height: 40rpx;
  font-size: 30rpx;
  border-bottom: 1rpx solid #eee;
}
.content .detail{
  width: 100%;
  height: 200rpx;
  overflow: hidden;
  font-size: 24rpx;
  padding: 20rpx;
  line-height: 40rpx;
}
.content image{
  width: 200rpx;
  height: 150rpx;
}